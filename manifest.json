{
  "manifest_version": 2,

  "name": "Porn Protector Plugin",
  "description": "This extension closes an active tab with porn content.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": [
   "tabs"
   ]
}